{
  "name": "euphoria",
  "version": "2.6.8",
  "description": "A euphoria CSS micro-framework.",
  "main": "lib/euphoria.cjs.js",
  "module": "lib/euphoria.esm.js",
  "ava": {
    "require": "babel-register"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "npm-run-all -p start-src start-test",
    "start-src": "rollup -c -w",
    "start-test": "npm run test -- --watch",
    "test": "ava src/**/*.test.js"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel": "6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "chalk": "2.3.0",
    "clean-css": "4.1.9",
    "euphoria-cli": "^2.6.5",
    "extract-text-webpack-plugin": "2.1.2",
    "glamor": "2.20.25",
    "html-webpack-harddisk-plugin": "0.1.0",
    "html-webpack-plugin": "2.28.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.9.2",
    "prop-types": "15.6.0",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "css-selector-parser": "1.3.0",
    "lightness": "1.0.0",
    "lodash": "4.17.4",
    "url-slug": "2.0.0"
  },
  "keywords": [
    "css",
    "sass",
    "scss",
    "euphoria",
    "glamor",
    "ui",
    "framework",
    "tool",
    "util",
    "utils",
    "utilties",
    "styling",
    "design",
    "bootstrap",
    "reset",
    "forms",
    "type",
    "tyopgraphy",
    "fonts",
    "font",
    "icon",
    "style"
  ],
  "author": "Dana Woodman <dana@danawoodman.com> (http://danawoodman.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danawoodman/euphoria/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/danawoodman/euphoria.git"
  },
  "homepage": "https://github.com/danawoodman/euphoria#readme"
}
